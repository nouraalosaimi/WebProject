      /* Header CSS starts */
      ul {
        list-style: none;
      }
      .header {
        display: flex;
        justify-content: space-between;
        align-items: center;
        position: relative;
        height: 70px;
        padding-left: 100px;
        padding-right: 100px;
        margin-left: auto;
        margin-right: auto;
      }
      header nav {
        flex: 1;
        display: flex;
        align-items: center;
        justify-content: flex-end;
      }
      nav ul {
        display: flex;
      }
      nav ul li a {
        padding: 40px 10px;
        color: #000000;
        text-decoration: none;
      }
      h1 span a {
        text-decoration: none;
        color: #000000;
      }
      nav ul li a:hover {
        color: #7ca4be;
      }
      /* Header CSS starts */

      /* Footer CSS starts */
      .footer {
        width: 60%;
        margin: auto;
        display: grid;
        grid-template-columns: repeat(3, 1fr);
      }
      .footer-wraper {
        margin-top: 30px;
      }
      .brand-container {
        display: flex;
        text-align: start;
        color: #4d5053;
      }
      .brand-container h1 {
        color: #000000;
      }
      .pages-container {
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
      }
      .contact-container {
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
        gap: 10px;
      }
      .content {
        display: flex;
        flex-direction: column;
        list-style-type: none;
        gap: 12px;
      }
      .content li a {
        text-decoration: none;
        color: #000000;
      }
      .copyright p {
        text-align: center;
        font-size: small;
      }
      .copyright p a {
        padding: 30px 0;
        text-align: center;
        color: #4d5053;
        text-decoration: none;
      }
      .copyright p a:hover {
        color: #7ca4be;
      }
      /* Footer CSS ends */

      .content h3 {
